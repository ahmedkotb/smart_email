{
  "name": "Smart Email",
  "version": "0.1",
  "description": "Smart Email Chrome Extension",
  "icons": {"128": "icon.png"},
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*", "http://mail.google.com/*"],
      "js": ["lib/bootstrap.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "http://mail.google.com/*",
    "https://mail.google.com/",
    "http://*/"
  ]
}
